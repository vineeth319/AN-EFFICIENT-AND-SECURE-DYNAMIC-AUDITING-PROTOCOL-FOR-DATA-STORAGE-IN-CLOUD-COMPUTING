<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegServlet</servlet-name>
        <servlet-class>com.action.RegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OwnerRegServlet</servlet-name>
        <servlet-class>com.action.OwnerRegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>O_Action</servlet-name>
        <servlet-class>com.action.O_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>U_Action</servlet-name>
        <servlet-class>com.action.U_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>com.upload.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Download1</servlet-name>
        <servlet-class>com.download.Download1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>View_Chart</servlet-name>
        <servlet-class>com.downloadgraph.View_Chart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Download</servlet-name>
        <servlet-class>com.download.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegServlet</servlet-name>
        <url-pattern>/RegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OwnerRegServlet</servlet-name>
        <url-pattern>/OwnerRegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>O_Action</servlet-name>
        <url-pattern>/O_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>U_Action</servlet-name>
        <url-pattern>/U_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download1</servlet-name>
        <url-pattern>/Download1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>View_Chart</servlet-name>
        <url-pattern>/View_Chart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/Download</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
